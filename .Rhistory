outlier <- function(x){
q1 <- quantile(x,0.25,na.rm=TRUE)
q3 <- quantile(x,0.75,na.rm=TRUE)
iqr <- IQR(x,na.rm=TRUE)
num_outliers <- 0
outliers <-  x <q1-(1.5*iqr)| x > q3+(1.5*iqr)
num_outliers <- sum(outliers,na.rm=TRUE)
if(num_outliers>0){
print(x[outliers])
} else{
print("no ")
}
return(num_outliers)
}
