# -------------------------------
#  Filter for Japan and selected indicators
# -------------------------------
japan_data <- data %>%
filter(
Country.Name == "Japan",
Indicator.Name %in% indicators
)
# -------------------------------
# Select only relevant columns (1990â€“2024)
# -------------------------------
japan_selected <- japan_data %>%
select(Country.Name, Indicator.Name, `X1990`:`X2024`)
# -------------------------------
#  Convert to long format for analysis
# -------------------------------
japan_long <- japan_selected %>%
pivot_longer(
cols = `X1990`:`X2024`,
names_to = "Year",
values_to = "Value"
)
# -------------------------------
#  (Optional) Spread to wide format per year for comparison
# -------------------------------
japan_wide <- japan_long %>%
pivot_wider(
names_from = Indicator.Name,
values_from = Value
)
# -------------------------------
#  View the result
# -------------------------------
head(japan_wide)
view(japan_wide)
# -------------------------------
#  Convert Year to numeric
# -------------------------------
japan_wide$Year <- as.numeric(gsub("X", "", japan_wide$Year))
# -------------------------------
#  Basic summary statistics
# -------------------------------
summary(japan_wide)
